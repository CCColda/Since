			"err": ":x: \"{creator}\" már létrehozta ezt az eseményt \"{date, date, ::yyyy-MM-dd}\" dátumra :(",
			"res": ":white_check_mark: Esemény létrehozva"
		},
		"since": {
			"desc": "Lekéri az időt egy eseménytől/eseményig",
			"label-prompt": "Mi az esemény címkéje?",
			"time": "{year, plural, =0 {} other {# év, }}{month} hónap, {day} nap, {hour} óra, {minute} perc, {second} másodperc",
			"until": "\"{label}\" eseményig `{time}` van hátra",
			"since": "\"{label}\" esemény óta `{time}` telt el.",
			"res": ":clock2: {for_or_until}",
			"err": ":x: Nem létezik ilyen esemény"
		},
		"remove": {
			"desc": "Eltávolít egy eseményt",
			"label-prompt": "Mi az esemény címkéje?",
